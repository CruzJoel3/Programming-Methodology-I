/* 
Write a program that reads in two integers and 
determines and prints whether the first is a 
multiple of the second
*/

#include <iostream>

using namespace std;

int main (){

int one;
int two;

cout << "Enter two integers: ";
cin >> one;
cin >> two;

if (one % two == 0)
{
    cout << one << " is a multiple of " << two << endl;
}

else
{
    cout << one << " is not a multiple of " << two << endl;
}
}
